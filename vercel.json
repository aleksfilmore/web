{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@18.x"
    }
  },
  "headers": [
    {
      "source": "/(.*\\.css)$",
      "headers": [
        {
          "key": "content-type",
          "value": "text/css"
        }
      ]
    }
  ]
}