{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "text/css; charset=utf-8"
      }
    },
    {
      "src": "/(.*\\.js)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "application/javascript; charset=utf-8"
      }
    },
    {
      "src": "/(.*\\.svg)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "image/svg+xml"
      }
    },
    {
      "src": "/(.*\\.png)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "image/png"
      }
    },
    {
      "src": "/(.*\\.jpg|.*\\.jpeg)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "image/jpeg"
      }
    },
    {
      "src": "/(.*\\.gif)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "image/gif"
      }
    },
    {
      "src": "/(.*\\.mp3)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "audio/mpeg"
      }
    },
    {
      "src": "/(.*\\.pdf)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "application/pdf"
      }
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml"
      }
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt",
      "headers": {
        "Content-Type": "text/plain"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}